{
  "manifest_version": 2,
  "name": "tb_goods_killing_countdown",
  "version": "1.0",
  "description": "淘宝抢购脚本（带倒计时）",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://cart.taobao.com/*","https://cart.tmall.com/*","https://buy.taobao.com/*","https://buy.tmall.com/*"],
      "js": ["tb_goods_killing.js"],
	    "run_at": "document_idle"
    }
  ]
}